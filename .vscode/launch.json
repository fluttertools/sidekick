      "name": "Flutter: Attach to Device",
      "type": "dart",
      "program": "lib/main.dart",
      "request": "attach"
    },
    {
      "name": "Flutter",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart"
    },
    {
      "name": "Flutter Profile",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "flutterMode": "profile"
    },
    {
      "name": "Integration Tests: Launch App",
      "request": "launch",
      "type": "dart",
      "program": "integration_test/app_test.dart"
    }
  ]
